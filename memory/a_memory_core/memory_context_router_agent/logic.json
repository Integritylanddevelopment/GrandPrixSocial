    "lazy_load_content": true,
    "parallel_search": false,
    "search_timeout_ms": 5000
  },
  "content_filtering": {
    "exclude_patterns": [
      "__pycache__",
      "*.tmp",
      "*.bak"
    ],
    "include_extensions": [
      ".txt",
      ".md",
      ".json"
    ],
    "max_file_size_kb": 500,
    "skip_empty_files": true
  },
