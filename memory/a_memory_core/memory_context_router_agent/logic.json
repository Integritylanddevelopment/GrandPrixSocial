        "medium",
        "low"
      ],
      "domain": [
        "frontend",
        "backend",
        "database",
        "deployment",
        "testing",
        "design"
      ],
      "complexity": [
        "simple",
        "moderate",
        "complex",
        "expert"
      ]
    },
    "semantic_generation": {
      "content_analysis_depth": "deep",
      "context_window_chars": 2000,
      "include_code_analysis": true,
      "include_error_patterns": true,
      "include_solution_patterns": true
    },
