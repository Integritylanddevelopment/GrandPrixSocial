    "fallback_to_partial_context": true,
    "retry_failed_searches": false,
    "log_search_failures": true
  },
  "search_patterns": {
    "exact_match_weight": 1.0,
    "partial_match_weight": 0.7,
    "semantic_match_weight": 0.5,
    "filename_match_weight": 0.8,
    "ai_tag_weight": 0.9,
    "structured_tag_weight": 0.8,
    "compound_tag_weight": 0.85
  },
  "ai_tagging_system": {
    "enabled": true,
    "tag_generation_model": "gpt-3.5-turbo",
    "embedding_model": "text-embedding-ada-002",
    "max_tags_per_file": 8,
    "min_semantic_similarity": 0.7,
    "tag_learning_enabled": true,
